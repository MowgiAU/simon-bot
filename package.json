{
  "name": "simon-bot",
  "version": "1.0.0",
  "description": "Modular Discord bot for FL Studio music producer community",
  "main": "dist/bot/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/bot/index.ts",
    "build": "npx prisma generate && tsc",
    "start": "tsx src/bot/index.ts",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "cd dashboard && npm run build",
    "api:dev": "tsx watch src/api/index.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "migrate": "prisma migrate dev",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "bot",
    "plugin",
    "modular"
  ],
  "author": "Simon Bot Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "discord.js": "^14.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.19.0",
    "lucide-react": "^0.563.0",
    "openai": "^6.17.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.10.0",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
